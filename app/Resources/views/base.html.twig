<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link type='text/css' href='{{ asset('assets/css/vendor.css') }}' rel='stylesheet' media='screen' />
    <link type='text/css' href='{{ asset('assets/css/app.css') }}' rel='stylesheet' media='screen' />
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
{% include 'modules/header.html.twig' %}

{% block body %}{% endblock %}

{% include 'modules/footer.html.twig' %}

<script src="{{ asset('assets/js/vendor.js') }}"></script>
<script src="{{ asset('assets/js/app.js') }}"></script>
{% block javascripts %}{% endblock %}
</body>
</html>

